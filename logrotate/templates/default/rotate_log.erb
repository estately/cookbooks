########################################################################
### NOTICE: this file managed by chef; changes made here will be lost.
########################################################################

<%= [@files].flatten.join(" ") %> {

    <% if @compress %>
    compress
    delaycompress
    <% end %>

    <% if @datestamp %>
    dateext
    <% end %>

    <%= @frequency %>
    rotate <%= @keep %>

    missingok
    notifempty

    <% if @postrotate %>
    sharedscripts
    postrotate
        <%= @postrotate %>
    endscript
    <% end %>

    <% if @copytruncate %>
    copytruncate
    <% end %>
}
